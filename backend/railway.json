{
  "$schema": "https://railway.app/schema.json",
  "build": {
    "builder": "DOCKERFILE",
    "dockerfilePath": "Dockerfile"
  },
  "deploy": {
    "numReplicas": 1,
    "restartPolicy": "ON_FAILURE",
    "sleepTimeout": 300,
    "limitMemory": "512M",
    "limitCPU": "1000m"
  },
  "domains": [
    {
      "name": "viviz-bulk-sender.up.railway.app"
    }
  ]
}
