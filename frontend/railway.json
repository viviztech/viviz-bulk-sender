{
  "$schema": "https://railway.app/schema.json",
  "build": {
    "builder": "NIXPACKS",
    "nixpacksPlan": {
      "build": {
        "cmd": ["npm install && npm run build"]
      }
    }
  },
  "deploy": {
    "numReplicas": 1,
    "restartPolicy": "ON_FAILURE"
  },
  "domains": [
    {
      "name": "viviz-bulk-sender-frontend.up.railway.app"
    }
  ]
}
